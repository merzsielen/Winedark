#version 430 corestruct Voxel{	uint	type;	uint	children;};layout (local_size_x = 1, local_size_y = 1, local_size_z = 1) in;layout(std430, binding = 3) buffer octreeBuffer{	Voxel voxels[];	};layout (binding = 1, rgba32f) uniform image2D imgOutput;void main(){	ivec2 coords = ivec2(gl_GlobalInvocationID.xy);	vec4 color = vec4(1.0f, 1.0f, 1.0f, 1.0f);	imageStore(imgOutput,coords,color);}